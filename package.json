{
	"name": "spotify-clone",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "jest --watch",
		"lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:js:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"lint:css": "stylelint **/*.{scss,css}",
		"lint:css:fix": "stylelint --fix **/*.{scss,css}",
		"lint:json": "prettier --check \"./**/*.json\"",
		"lint:json:fix": "prettier --write \"./**/*.json\"",
		"lint": "npm run lint:js && npm run lint:css && npm run lint:json",
		"lint:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:json:fix",
		"commit": "commit",
		"prepare": "husky install",
		"lint-staged": "npx lint-staged --concurrent false"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"autoprefixer": "10.4.14",
		"eslint": "8.39.0",
		"express": "^4.18.2",
		"husky": "^8.0.3",
		"next": "13.3.2",
		"postcss": "8.4.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-query": "^3.39.3",
		"redux": "^4.2.1",
		"stylelint": "^15.6.0",
		"stylelint-checkstyle-formatter": "^0.1.2",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-scss": "^5.0.0",
		"tailwindcss": "3.3.2",
		"typescript": "5.0.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@commitlint/prompt-cli": "^17.6.1",
		"@iconify/react": "^4.1.0",
		"@next/eslint-plugin-next": "^13.3.2",
		"@types/node": "^18.16.3",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.1",
		"@types/react-query": "^1.2.9",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint-config-next": "^13.3.2",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"lint-staged": "^13.2.2",
		"prettier": "2.8.8"
	}
}
